function validaCampo(field, label, validacaoExtra, fieldName) {
	// Checkbox / Radio
	if (field == undefined) {
		validaChecked	= false;
		
		for(x=0;x<form.elements.length;x++)
			if (((form.elements[x].name == fieldName) || (form.elements[x].name == fieldName+"[]")) && (form.elements[x].checked))
					validaChecked = true;
		
		if (!validaChecked) {
			alert(str_decode("O campo '" + label + "' &eacute; obrigat&oacute;rio."));
			return(false);
		}
	}
	
	// Texto
	else if ((field.type == "text") || (field.type == "textarea") || (field.type == "select-one") || (field.type == "hidden")) {
		if (!field.value) {
			alert(str_decode("O campo '" + label + "' &eacute; obrigat&oacute;rio."));
			return(false);
		}
		if (validacaoExtra == 1)
			return mascaraData(field.value,field,label);
	}


	// Preenchido
	return(true);
}





function isAValidDate(date) {
	var ardt	= new Array;
	var ExpReg	= new RegExp("(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/[12][0-9]{3}");
	ardt		= date.split("/");
	erro		= true;
	if ( date.search(ExpReg)==-1)
		erro = false;
	else if (((ardt[1]==4)||(ardt[1]==6)||(ardt[1]==9)||(ardt[1]==11))&&(ardt[0]>30))
		erro = false;
	else if ( ardt[1]==2) {
		if ((ardt[0]>28)&&((ardt[2]%4)!=0))
			erro = false;
		if ((ardt[0]>29)&&((ardt[2]%4)==0))
			erro = false;
	}
	return erro;
}

function isAValidCPF(strCPF) {
    var Soma;
    var Resto;
    
    Soma = 0;
    strCPF = strCPF.toString().replace(/\./gi, "");
    strCPF = strCPF.replace(/-/gi, "");
    if (strCPF == "00000000000") return false;
     
    for (i=1; i<=9; i++) Soma = Soma + parseInt(strCPF.substring(i-1, i)) * (11 - i);
    Resto = (Soma * 10) % 11;
     
    if ((Resto == 10) || (Resto == 11))  Resto = 0;
    if (Resto != parseInt(strCPF.substring(9, 10)) ) return false;
     
    Soma = 0;
    for (i = 1; i <= 10; i++) Soma = Soma + parseInt(strCPF.substring(i-1, i)) * (12 - i);
    Resto = (Soma * 10) % 11;
     
    if ((Resto == 10) || (Resto == 11))  Resto = 0;
    if (Resto != parseInt(strCPF.substring(10, 11) ) ) return false;
    return true;
}

function isAValidCNPJ(cnpj){
    var valida = new Array(6,5,4,3,2,9,8,7,6,5,4,3,2);
    var dig1= new Number;
    var dig2= new Number;
    
    exp = /\.|\-|\//g
    cnpj = cnpj.toString().replace( exp, "" ); 
    var digito = new Number(eval(cnpj.charAt(12)+cnpj.charAt(13)));
            
    for(i = 0; i<valida.length; i++){
            dig1 += (i>0? (cnpj.charAt(i-1)*valida[i]):0);  
            dig2 += cnpj.charAt(i)*valida[i];       
    }
    dig1 = (((dig1%11)<2)? 0:(11-(dig1%11)));
    dig2 = (((dig2%11)<2)? 0:(11-(dig2%11)));
    
    if(((dig1*10)+dig2) != digito)  
            return false;
    return true;            
}

function findPosX(obj){
    var curleft = 0;
    if(obj.offsetParent) {
        while(1){
            curleft += obj.offsetLeft;
            if(!obj.offsetParent)
                break;
            obj = obj.offsetParent;
        }
    }
    else if(obj.x)
        curleft += obj.x;
    return curleft;
}

